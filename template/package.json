{
  "name": "TempApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "link-assets": "react-native-asset",
    "reverse": "adb start-server && adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "pods": "pod repo update && npx pod-install",
    "android:dev": "react-native run-android --variant=developmentdebug",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "apk:dev": "cd android && ./gradlew assembleDevelopment && cd ..",
    "apk:staging": "cd android && ./gradlew assembleStaging && cd ..",
    "apk:prod": "cd android && ./gradlew assembleProduction && cd ..",
    "bundle:dev": "cd android && ./gradlew bundleDevelopment && cd ..",
    "bundle:staging": "cd android && ./gradlew bundleStaging && cd ..",
    "bundle:prod": "cd android && ./gradlew bundleProduction && cd ..",
    "ios-clean": "cd ios && rm -R build && xcodebuild clean && cd ..",
    "ios:dev": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Debug\"",
    "ios:dev-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Debug\"",
    "ios:dev-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (5th generation)\" --scheme \"Debug\"",
    "ios:staging": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Staging\"",
    "ios:staging-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Staging\"",
    "ios:staging-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (5th generation)\" --scheme \"Staging\"",
    "ios:prod": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Release\"",
    "ios:prod-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Release\"",
    "ios:prod-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (5th generation)\" --scheme \"Release\"",
    "ios:dev-release": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Debug\" --configuration Release",
    "ios:staging-release": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Staging\" --configuration Release",
    "ios:prod-release": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Release\" --configuration Release",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "yarn pods && yarn link-assets && yarn android-clean && yarn ios-clean",
    "svg": "yarn react-native-vector-image generate"
  },
  "dependencies": {
    "@klarna/react-native-vector-drawable": "^0.3.0",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-native-firebase/analytics": "14.11.1",
    "@react-native-firebase/app": "14.11.1",
    "@react-native-firebase/crashlytics": "14.11.1",
    "@react-native-firebase/messaging": "14.11.1",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.2",
    "i18n-js": "^3.9.2",
    "lodash.memoize": "^4.1.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.0.0",
    "react-native": "0.69.0",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^9.0.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localize": "^2.2.2",
    "react-native-mmkv-storage": "^0.7.6",
    "react-native-paper": "^4.12.2",
    "react-native-progress": "^5.0.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-responsive-dimensions": "^3.1.1",
    "react-native-restart": "^0.0.24",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.13.1",
    "react-native-size-matters": "^0.4.0",
    "react-native-svg": "^12.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-vector-image": "^0.3.3",
    "react-redux": "^8.0.2",
    "redux-logger": "^3.0.6",
    "roqay-react-native-common-components": "^1.3.1",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/runtime": "^7.18.3",
    "@react-native-community/eslint-config": "^3.0.3",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^28.1.3",
    "@types/lodash.memoize": "^4.1.7",
    "@types/react-native": "^0.69.0",
    "@types/react-native-push-notification": "^8.1.1",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^28.1.1",
    "eslint": "^8.18.0",
    "jest": "^28.1.1",
    "metro-react-native-babel-preset": "^0.71.1",
    "react-test-renderer": "18.0.0",
    "reactotron-react-native": "^5.0.2",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
